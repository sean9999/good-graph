{
  "name": "good-graph",
  "version": "1.0.1",
  "description": "Good Graph is a pretty good graph",
  "main": "main.js",
  "type": "module",
  "sideEffects": false,
  "private": false,
  "scripts": {
    "start": "parcel www/src/index.html",
    "build:parcel": "parcel build --target=demo --no-source-maps --no-scope-hoist",
    "copy-assets": "cd www && mkdir -p dist/assets/css && cp -rf src/css/*.css dist/assets/css/ && cp -f src/favicon.ico dist/",
    "build:all": "npm run build:parcel && npm run copy-assets",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "targets": {
    "rc": {
      "source": "./www/src/index.html",
      "distDir": "./www/dist/rc"
    },
    "demo": {
      "source": "./www/src/index.html",
      "distDir": "./www/dist"
    }
  },
  "staticFiles": {
    "staticPath": "www/src/css",
    "staticOutPath": "css"
  },
  "browserslist": [
    "defaults, not dead, last 2 versions"
  ],
  "author": "@code_monk",
  "license": "ISC",
  "devDependencies": {
    "parcel": "^2.12.0"
  },
  "dependencies": {
    "@swc/helpers": "^0.5.12",
    "force-graph": "^1.43.5"
  }
}
